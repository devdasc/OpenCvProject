    "%config IPCompleter.greedy=True\n",
    "import cv2\n",
    "capture = cv2.VideoCapture(0)\n",
    "while (True):\n",
    "    ret, frame = capture.read()\n",
    "    cv2.imshow('Frame',frame)\n",
    "    keypress = cv2.waitKey(1)\n",
    "    if keypress ==ord('q'):\n",
    "        break\n",
    "capture.release()\n",
    "cv2.destroyAllWindows()\n",
    "    "
